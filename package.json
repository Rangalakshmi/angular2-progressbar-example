{
    "name": "angular2-progressbar-example",
    "version": "1.0.0",
    "description": "Angular 2 Progressbar example",
    "main": "index.js",
    "scripts": {
        "postinstall": "npm run typings install",
        "build": "tsc",
        "build:w": "tsc --watch",
        "bundle": "node bundle.js",
        "bundle:prod": "node bundle.js --prod",
        "lite": "lite-server",
        "start": "concurrently \"npm run build:w\" \"npm run lite\" ",
        "typings": "typings"
    },
    "dependencies": {
        "@angular/common": "2.0.0",
        "@angular/compiler": "2.0.0",
        "@angular/core": "2.0.0",
        "@angular/forms": "2.0.0",
        "@angular/http": "2.0.0",
        "@angular/platform-browser": "2.0.0",
        "@angular/platform-browser-dynamic": "2.0.0",
        "@angular/router": "3.0.0",

        "angular2-progressbar": "^0.5.1",
        "progressbar.js": "^1.0.1",

        "lodash": "^4.13.1",

        "systemjs": "0.19.27",
        "core-js": "^2.4.1",
        "reflect-metadata": "^0.1.3",
        "rxjs": "5.0.0-beta.12",
        "zone.js": "^0.6.23"
    },
    "devDependencies": {
        "concurrently": "^2.0.0",
        "lite-server": "^2.2.0",
        "typescript": "1.8.10",
        "typings": "^1.0.4",
        "systemjs-builder": "^0.15.26",
        "yargs": "^4.8.1"
    },
    "author": "Kapil Sachdeva",
    "license": "MIT"
}
